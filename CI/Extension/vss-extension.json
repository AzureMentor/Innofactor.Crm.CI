{
  "manifestVersion": 1,
  "id": "cinteros-devutils-ci-build-tasks",
  "name": "DevOps for Microsoft Dynamics 365",
  "version": "8.3.14",
  "publisher": "InnofactorSE",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Build and Release tasks to facilitate a complete DevOps approach to Microsoft Dynamics 365 projects using Innofactor Shuffle and related tools.",
  "categories": [
    "Build and release"
  ],
  "galleryFlags": [
    "Preview",
    "Public"
  ],
  "tags": [
    "Dynamics 365",
    "Dynamics CRM",
    "CRM",
    "Shuffle",
    "CRM 365"
  ],
  "icons": {
    "default": "Images/InnofactorIcon128.png",
    "large": "Images/InnofactorIcon512.png"
  },
  "screenshots": [
    {
      "path": "Images/Tasks.png"
    },
    {
      "path": "Images/Minify.png"
    },
    {
      "path": "Images/WebResources.png"
    },
    {
      "path": "Images/Assembly.png"
    },
    {
      "path": "Images/Export.png"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "learn": {
      "uri": "http://jonasrapp.innofactor.se/2017/04/DevOps-I.html"
    },
    "repository": {
      "uri": "https://github.com/Innofactor/Innofactor.Crm.CI"
    },
    "issues": {
      "uri": "https://github.com/Innofactor/Innofactor.Crm.CI/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Innofactor/Innofactor.Crm.CI"
  },
  "branding": {
    "color": "rgb(0, 93, 164)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "Images",
      "addressable": true
    },
    {
      "path": "ApplyVersionToAssemblies"
    },
    {
      "path": "ShuffleExport"
    },
    {
      "path": "ShuffleImport"
    },
    {
      "path": "UpdateAssembly"
    },
    {
      "path": "UpdateWebResources"
    },
    {
      "path": "MinifyJS"
    }
  ],
  "contributions": [
    {
      "id": "ApplyVersionToAssemblies",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "ApplyVersionToAssemblies"
      }
    },
    {
      "id": "ShuffleExport",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "ShuffleExport"
      }
    },
    {
      "id": "ShuffleImport",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "ShuffleImport"
      }
    },
    {
      "id": "UpdateAssembly",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "UpdateAssembly"
      }
    },
    {
      "id": "UpdateWebResources",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "UpdateWebResources"
      }
    },
    {
      "id": "MinifyJS",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "MinifyJS"
      }
    }
  ]
}
