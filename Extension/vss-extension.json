      ],
      "properties": {
        "name": "ObfuscateAssembly"
      }
    },
    {
      "id": "ShuffleExport",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "ShuffleExport"
      }
    },
    {
      "id": "ShuffleImport",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
