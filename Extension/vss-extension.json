{
  "manifestVersion": 1,
  "id": "cinteros-devutils-ci-build-tasks",
  "name": "DevOps for Microsoft Dynamics 365",
  "version": "9.0.9",
  "publisher": "InnofactorSE",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Build and Release tasks to facilitate a complete DevOps approach to Microsoft Dynamics 365 projects using Innofactor Shuffle and related tools.",
  "categories": [
    "Build and release"
  ],
  "galleryFlags": [
    "Public"
  ],
  "tags": [
    "Dynamics 365",
    "Dynamics CRM",
    "CRM",
    "Shuffle",
    "CRM 365",
    "Obfuscation"
  ],
  "icons": {
    "default": "icon128x128.png",
    "large": "icon512x512.png"
  },
  "screenshots": [
    {
      "path": "documentation/images/tasks.png"
    },
    {
      "path": "documentation/images/minify.png"
    },
    {
      "path": "documentation/images/resources.png"
    },
    {
      "path": "documentation/images/assembly.png"
    },
    {
      "path": "documentation/images/export.png"
    }
  ],
  "content": {
    "details": {
      "path": "documentation/overview.md"
    }
  },
  "links": {
    "learn": {
      "uri": "http://jonasrapp.innofactor.se/2017/04/DevOps-I.html"
    },
    "repository": {
      "uri": "https://github.com/Innofactor/Innofactor.Crm.CI"
    },
    "issues": {
      "uri": "https://github.com/Innofactor/Innofactor.Crm.CI/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/Innofactor/Innofactor.Crm.CI"
  },
  "branding": {
    "color": "rgb(0, 93, 164)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "documentation\\images",
      "addressable": true
    },
    {
      "path": "implementation\\ApplyVersionToAssemblies"
    },
    {
      "path": "implementation\\ObfuscateAssembly"
    },
    {
      "path": "implementation\\ShuffleExport"
    },
    {
      "path": "implementation\\ShuffleImport"
    },
    {
      "path": "implementation\\UpdateAssembly"
    },
    {
      "path": "implementation\\UpdateWebResources"
    },
    {
      "path": "implementation\\MinifyJS"
    },
    {
      "path": "implementation\\WhoAmI"
    }
  ],
  "contributions": [
    {
      "id": "ApplyVersionToAssemblies",
      "type": "ms.vss-distributed-task.task",
